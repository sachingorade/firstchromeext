{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows Unread mail count and info of gmail.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"notifications",
	"alarms"
  ]
}